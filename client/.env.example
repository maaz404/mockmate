# Client .env example
"Copy this file to client/.env and replace placeholder values."
# Do not include inline comments on the same line as a variable value.

# Google OAuth (handled on the server; client may not need this)
REACT_APP_GOOGLE_CLIENT_ID=

# API (server) endpoints
REACT_APP_API_URL=http://localhost:5000
REACT_APP_API_BASE_URL=http://localhost:5000/api

# App metadata
REACT_APP_APP_NAME=MockMate
REACT_APP_VERSION=1.0.0
