# Client .env example
# Copy this file to client/.env and replace placeholder values with your keys.
# Do not include inline comments on the same line as a variable value.

# Clerk (frontend) - publishable key from your Clerk dashboard
REACT_APP_CLERK_PUBLISHABLE_KEY=pk_test_YOUR_PUBLISHABLE_KEY_HERE

# API (server) endpoints
REACT_APP_API_URL=http://localhost:5000
REACT_APP_API_BASE_URL=http://localhost:5000/api

# App metadata
REACT_APP_APP_NAME=MockMate
REACT_APP_VERSION=1.0.0
