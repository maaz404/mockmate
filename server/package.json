{
  "name": "mockmate-server",
  "version": "1.0.0",
  "description": "MockMate backend server",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "seed": "node src/scripts/seedQuestions.js",
    "seed:dev": "node src/scripts/seedDevAtlas.js",
    "prod": "NODE_ENV=production node src/server.js",
    "clean": "rm -rf node_modules && npm install"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "cloudinary": "^1.41.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "groq-sdk": "^0.7.0",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.1",
    "openai": "^4.104.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pdfkit": "^0.17.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "eslint-config-node": "^4.1.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.2.1",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "express",
    "mongodb",
    "jwt",
    "api"
  ],
  "author": "Maaz Sheikh",
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testMatch": [
      "**/src/__tests__/**/*.test.js"
    ],
    "verbose": false
  },
  "license": "MIT"
}
